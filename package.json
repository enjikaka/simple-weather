{
  "name": "simple-weather",
  "version": "3.0.2",
  "description": "Simple Weather browser extension",
  "scripts": {
    "css-settings": "./node_modules/.bin/postcss --config postcss.settings.config.json",
    "css-popup": "./node_modules/.bin/postcss --config postcss.popup.config.json",
    "css-weather-dashboard": "./node_modules/.bin/postcss --config postcss.weather-dashboard.config.json",
    "css": "npm run css-weather-dashboard && npm run css-popup && npm run css-settings",
    "test": "semistandard",
    "start": "node index",
    "build": "npm run css"
  },
  "keywords": [
    "map"
  ],
  "author": "Jeremy Karlsson",
  "license": "UNLICENSED",
  "devDependencies": {
    "cssnano": "^3.5.2",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.5.2",
    "postcss-cli": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-reporter": "^1.3.3"
  }
}
